                        <tr class="rowClass" data-bind="css: { 'success': status() == 'Confirmed', 'info': status().toLowerCase() == 'postponed', 'danger': status().toLowerCase() == 'not_confirmed'}">
                            <!--td data-bind="text: appointmentID"></td>-->
                            <td data-bind="text: $index() + 1"></td>
                            <td data-bind="text: surname"></td>
                            <td data-bind="text: medical_Type"></td>
                            <td data-bind="text: doctor"></td>
                            <td data-bind="text: dateTime"></td>
                            <td data-bind="template: { name: whichTemplate, if: whichTemplate }"></td>
                            <td>
