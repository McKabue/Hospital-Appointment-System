                                        <li data-bind="visible: roleName() == 'DOCTOR'"><a href="#">Urgent (see now)</a></li>
                                        <li data-bind="visible: roleName() == 'DOCTOR', click: $root.postPone"><a href="#">PostPone</a></li>
                                        <li><a href="#">Select</a></li>
                                        <li><a href="#" data-bind="click: $root.deleteData">Delete</a></li>
                                    </ul>
                                </div>
                                <span style="padding-left:5px; font-size:1.5em;"> <input class="largerCheckbox" style="font-size:1.5em;" type="checkbox"> </span>
